grade = int(input("Your Grade"))
if grade >= 100 :
  print("A")
elif grade >= 90 :
  print ("B")
elif grade >= 80 :
  print("C")
elif grade >= 70 :
  print("D")
elif grade >= 60 :
  print("F")

